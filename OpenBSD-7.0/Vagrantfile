require "yaml"

Vagrant.configure("2") do |config|
  config.vm.define "virtualbox" do |virtualbox|
    virtualbox.vm.hostname = "virtualbox-OpenBSD-7.0"
    virtualbox.vm.box = "file://virtualbox.box"

    config.vm.provider :virtualbox do |v|
      v.gui = false
      v.name = "virtualbox-OpenBSD-7.0"
    end
  end
end
